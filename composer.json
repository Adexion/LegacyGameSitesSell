{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "google/recaptcha": "*",
        "guzzlehttp/guzzle": "*",
        "monolog/monolog": "*",
        "nelmio/api-doc-bundle": "*",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "*",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "*",
        "symfony/orm-pack": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "*",
        "symfony/swiftmailer-bundle": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "behat/behat": "*",
        "friends-of-behat/symfony-extension": "*",
        "imbo/behat-api-extension": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "./bin"
    },
    "autoload": {
        "psr-4": {
            "ModernGame\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ModernGame\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "*"
        }
    }
}
